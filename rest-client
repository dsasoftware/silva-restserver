#!/bin/bash
D=`date -u +'Superhemlig nyckel%Y-%m-%d %H:%M:%S'`
D=${D:0:${#D}-1}
echo $D
APPKEY=`echo -n $D|sha256sum|cut -d ' ' -f 1`
echo $APPKEY
#127.0.0.1
#verdandi.silva.se
curl -H "Content-type: application/json" -X POST http://verdandi.silva.se:5000/inventory -d '{"appkey":"'${APPKEY}'","articles":{"1337":59,"1339":1}}' -i
curl -H "Content-type: application/json" -X POST http://verdandi.silva.se:5000/order -d '{"appkey": "'${APPKEY}'", "order": 23}' -i
echo $APPKEY
