#!/bin/bash
D=`date -u +'Superhemlig nyckel%Y-%m-%d %H:%M'`
echo $D
APPKEY=`echo -n $D|sha256sum|cut -d ' ' -f 1`
echo $APPKEY
#127.0.0.1
#verdandi.silva.se
curl -H "Content-type: application/json" -X POST http://verdandi.silva.se:5000/inventory -d '{"appkey":"'${APPKEY}'","articles":[{"ItemId":"21120","Quantity":1}]}' -i
curl -H "Content-type: application/json" -X POST http://verdandi.silva.se:5000/order -d '{"appkey": "'${APPKEY}'", "order": {"head":{"OrderIdWeb": "123"},"rows":[{"RowIdWeb": "52", "ItemId": "21120", "Quantity": 3}]}}' -i
curl -H "Content-type: application/json" -X POST http://verdandi.silva.se:5000/order_info -d '{"appkey": "'${APPKEY}'", "order": {"OrderIdWeb": "123"}}' -i
echo $APPKEY
